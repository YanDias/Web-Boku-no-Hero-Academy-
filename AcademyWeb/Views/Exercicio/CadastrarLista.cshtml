@model ListaTreino

@{
    
}
<center><h2>Cadastrar Lista Exercicios</h2></center>
<br /><br />
    <form asp-action="CadastrarLista" method="post"
          enctype="multipart/form-data">
        <fieldset>
            <legend>Exercicios</legend>
            <div asp-validation-summary="ModelOnly"
                 class="text-primary"></div>
            <div class="form-group" id="NomeLista">
                <label asp-for="NomeLista"></label>
                <input type="text" class="form-control"
                       asp-for="NomeLista" />
                <span asp-validation-for="NomeLista"
                      class="text-primary"></span>
            </div>

            <div class="form-group" id="Dsemana">
                <label asp-for="DSemana"></label>
                <input type="text" class="form-control"
                       asp-for="DSemana" />
                <span asp-validation-for="DSemana"
                      class="text-primary"></span>
            </div>
            <div>
                <input type="submit" class="btn btn-success" value="Cadastrar" />
                <br><br />

            </div>

        </fieldset>
    </form>